{
    "name": "streams/core",
    "description": "A domain-driven, code-configured, flat-file application platform for Laravel.",
    "authors": [{
        "name": "Ryan Thompson",
        "email": "ryan@pyrocms.com"
    }],
    "require": {
        "misd/linkify": "1.1.*",
        "fideloper/proxy": "^4.0",
        "jenssegers/agent": "^2.6",
        "mrclay/jsmin-php": "^2.4",
        "erusev/parsedown": "^1.7",
        "tmarois/filebase": "^1.0",
        "tubalmartin/cssmin": "^4.0",
        "laravel/scout": "^8.0|^9.0",
        "intervention/image": "^2.5",
        "ezyang/htmlpurifier": "^4.0",
        "laravel/framework": "^8.0|^9.0",
        "erusev/parsedown-extra": "^0.7",
        "nicmart/string-template": "^0.1",
        "spatie/yaml-front-matter": "^2.0",
        "laravelcollective/html": "^6.1.0"
    },
    "require-dev": {
        "fzaninotto/faker": "^1.9",
        "mockery/mockery": "^1.3",
        "phpunit/phpunit": "^9.0",
        "orchestra/testbench": "^5.3",
        "helmich/phpunit-json-assert": "^3.1"
    },
    "autoload": {
        "psr-4": {
            "Anomaly\\Streams\\Platform\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": ["tests/"]
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Anomaly\\Streams\\Platform\\StreamsServiceProvider"
            ]
        }
    },
    "license": "MIT"
}
